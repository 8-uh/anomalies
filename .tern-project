{
  "ecmaVersion": "7",
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "node_modules/react*/**/*",
    "node_modules/*code*mirror*/**/*"
  ],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "doc_comment": true,
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {},
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {},
    "commonjs": {}
  }
}